<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="ff0500" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="ff0500" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="false" pattern="\baskbus\b.*">
      <option name="FOREGROUND" value="cdff00" />
      <option name="BACKGROUND" value="161616" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="ff0500" />
      <option name="ERROR_STRIPE_COLOR" value="55ff" />
    </pattern>
  </component>
</application>